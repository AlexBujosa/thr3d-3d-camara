{
  "compilerOptions": {
    "target": "ES6",    
    "module": "ES2015",   
    "outDir": "./out",
    "rootDir": "./src",
    "esModuleInterop": true,       
    "forceConsistentCasingInFileNames": true,      
    "strict": true,                       
    "skipLibCheck": true                                 /* Skip type checking all .d.ts files. */
  },
  "include":["src"]
}
